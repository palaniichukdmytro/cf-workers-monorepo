{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "useDefineForClassFields": true,
    "incremental": true,
    "jsx": "preserve",
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ]
  }
}
